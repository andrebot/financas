{
  "name": "financas",
  "version": "1.0.0",
  "description": "App to help me keep up with my finances",
  "main": "index.js",
  "engine": {
    "node": "^19.9.0"
  },
  "scripts": {
    "server-up": "nodemon --exec ts-node src/server/index.ts",
    "build-client": "webpack --config webpack/webpack.prod.js",
    "build-client:dev": "webpack --config webpack/webpack.dev.js",
    "build-client:watch": "webpack --config webpack/webpack.dev.js --watch",
    "lint:all": "eslint 'src/server/**/*.ts' 'src/client/**/*.tsx'",
    "lint:all:fix": "eslint --fix 'src/server/**/*.ts' 'src/client/**/*.tsx'",
    "lint:server": "eslint src/server/**/*.ts",
    "lint:client": "eslint src/client/**/*.tsx",
    "lint:server:fix": "eslint --fix src/server/**/*.ts",
    "lint:client:fix": "eslint --fix src/client/**/*.tsx",
    "create-route": "node scripts/createRoute.js",
    "test:server": "NODE_ENV=test nyc mocha -r ts-node/register 'tests/server/**/*.spec.ts' --exit",
    "test:server:integration": "NODE_ENV=test mocha -r ts-node/register --file ./tests/integration/testSetup.ts 'tests/integration/**/*.spec.ts' --exit",
    "test:client": "jest --coverage",
    "test:all": "npm run test-server && npm run test-client",
    "test:server-file": "NODE_ENV=test node tests/runTestFile.js",
    "dev": "npm-run-all --parallel build-client:watch server-up"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/server/**/*.ts"
    ],
    "exclude": [
      "tests/server/**/*.ts",
      "src/server/index.ts",
      "src/server/server.ts",
      "src/server/utils/logger.ts",
      "src/server/config/*",
      "src/server/public/*"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "author": "Andre Almeida",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.19",
    "@mui/x-date-pickers": "^6.18.2",
    "@reduxjs/toolkit": "^1.9.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "export-config": "^1.0.4",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "i18next": "^23.7.11",
    "i18next-browser-languagedetector": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.2",
    "node-fetch": "^3.3.2",
    "notistack": "^3.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@emotion/babel-plugin": "^11.11.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^4.3.11",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@types/proxyquire": "^1.3.31",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/sinon": "^17.0.2",
    "@types/sinon-chai": "^3.2.12",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "babel-loader": "^9.1.3",
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "chokidar": "^3.6.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^9.1.6",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
}
