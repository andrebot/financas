{
  "name": "financas",
  "version": "1.0.0",
  "description": "App to help me keep up with my finances",
  "main": "index.js",
  "engine": {
    "node": "^22.14.0"
  },
  "scripts": {
    "server-up": "nodemon --exec ts-node src/server/index.ts",
    "server-debug": "nodemon",
    "build-client": "webpack --config webpack/webpack.prod.js",
    "build-client:dev": "webpack --config webpack/webpack.dev.js",
    "build-client:watch": "webpack --config webpack/webpack.dev.js --watch",
    "lint:all": "eslint 'src/server/**/*.ts' 'src/client/**/*.tsx'",
    "lint:all:fix": "eslint --fix 'src/server/**/*.ts' 'src/client/**/*.tsx'",
    "lint:server": "eslint src/server/**/*.ts",
    "lint:client": "eslint src/client/**/*.tsx",
    "lint:server:fix": "eslint --fix src/server/**/*.ts",
    "lint:client:fix": "eslint --fix src/client/**/*.tsx",
    "create-route": "node scripts/createRoute.js",
    "create-page": "node scripts/createPage.js",
    "test:e2e:report": "playwright show-report ./playwright-report",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e": "playwright test",
    "test:server": "NODE_ENV=test nyc mocha -r ts-node/register 'tests/server/**/*.spec.ts' --exit",
    "test:server:integration": "NODE_ENV=test mocha -r ts-node/register --file ./tests/integration/testSetup.ts 'tests/integration/**/*.spec.ts' --exit",
    "test:client": "jest --coverage",
    "test:all": "npm run test:server && npm run test:server:integration && npm run test:client",
    "test:server-file": "NODE_ENV=test node tests/runTestFile.js",
    "dev": "NODE_ENV=development npm-run-all --parallel build-client:watch server-debug",
    "test:client:file": "jest --watch",
    "postinstall": "playwright install-deps && playwright install"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/server/**/*.ts"
    ],
    "exclude": [
      "tests/server/**/*.ts",
      "src/server/index.ts",
      "src/server/server.ts",
      "src/server/utils/logger.ts",
      "src/server/config/*",
      "src/server/public/*",
      "node_modules/**"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "exclude-node-modules": true
  },
  "author": "Andre Almeida",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "@mui/x-date-pickers": "^7.28.0",
    "@reduxjs/toolkit": "^2.6.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "export-config": "^1.0.5",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lcov-summary": "^1.0.1",
    "mongodb": "^6.15.0",
    "mongoose": "^8.13.0",
    "node-fetch": "^3.3.2",
    "notistack": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-redux": "^9.2.0",
    "react-router": "^7.4.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@emotion/babel-plugin": "^11.13.5",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^5.2.1",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.14",
    "@types/proxyquire": "^1.3.31",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/sinon": "^17.0.4",
    "@types/sinon-chai": "^4.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "babel-loader": "^10.0.0",
    "chai": "^5.2.0",
    "chokidar": "^4.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mocha": "^11.1.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "nyc": "^17.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^20.0.0",
    "sinon-chai": "^4.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.3.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.1",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^6.0.1",
    "whatwg-fetch": "^3.6.20"
  }
}
